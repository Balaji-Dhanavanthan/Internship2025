{
	"name": "PL_04",
	"properties": {
		"activities": [
			{
				"name": "develop",
				"type": "GetMetadata",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"dataset": {
						"referenceName": "DS_Blob",
						"type": "DatasetReference",
						"parameters": {
							"Container": "practice-adf",
							"Directory": "aabidh",
							"File": "countries.csv"
						}
					},
					"fieldList": [
						"itemName"
					],
					"storeSettings": {
						"type": "AzureBlobStorageReadSettings",
						"recursive": true,
						"enablePartitionDiscovery": false
					},
					"formatSettings": {
						"type": "DelimitedTextReadSettings"
					}
				}
			},
			{
				"name": "Filter1",
				"type": "Filter",
				"dependsOn": [
					{
						"activity": "develop",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@activity('develop').output.itemName",
						"type": "Expression"
					},
					"condition": {
						"value": "@equals(item().name,'countries.csv')",
						"type": "Expression"
					}
				}
			}
		],
		"parameters": {
			"folder": {
				"type": "array"
			},
			"directory": {
				"type": "array"
			},
			"file": {
				"type": "array"
			}
		},
		"annotations": []
	}
}