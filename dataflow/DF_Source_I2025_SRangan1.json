{
	"name": "DF_Source_I2025_SRangan1",
	"properties": {
		"folder": {
			"name": "DF_TDSG_Project_Scenario"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DS_Blob",
						"type": "DatasetReference"
					},
					"name": "SrcPlan"
				},
				{
					"dataset": {
						"referenceName": "DS_STTM_I2025_SRangan1",
						"type": "DatasetReference"
					},
					"name": "SrcMaterial"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DS_Source_I2025_SRangan1",
						"type": "DatasetReference"
					},
					"name": "SNKMaterial"
				},
				{
					"dataset": {
						"referenceName": "DS_Source_I2025_SRangan1",
						"type": "DatasetReference"
					},
					"name": "SNKPlan"
				}
			],
			"transformations": [
				{
					"name": "DCnulltoString"
				},
				{
					"name": "DCtoDate"
				},
				{
					"name": "DCEnddatetoDate"
				}
			],
			"scriptLines": [
				"source(output(",
				"          MaterialCode as string,",
				"          ForecastDate as string,",
				"          Plan as short,",
				"          TrialProduct as string,",
				"          IsActive as string,",
				"          RevisionNumber as integer,",
				"          RevisionComment as string,",
				"          Line as string,",
				"          CreatedOn as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> SrcPlan",
				"source(output(",
				"          MaterialCode as string,",
				"          CreatedDate as string,",
				"          MaterialDescription as string,",
				"          ProductVarient as string,",
				"          ProductType as string,",
				"          CustomerMaster as string,",
				"          Line as string,",
				"          ProductionLocation as string,",
				"          ProductionPlant as string,",
				"          {SalesPlant-GJ} as string,",
				"          {SalesPlant-HR} as string,",
				"          {SalesPlant-KA} as string,",
				"          IsActive as string,",
				"          StartDate as string,",
				"          EndDate as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> SrcMaterial",
				"SrcMaterial derive(each(match(type=='string'), $$ = case($$=='null', toString(null()),$$))) ~> DCnulltoString",
				"SrcPlan derive(CreatedOn = case(CreatedOn=='getdate()',currentDate(), toDate(null())),",
				"          ForecastDate = toDate(ForecastDate, 'dd-MM-yyyy')) ~> DCtoDate",
				"DCnulltoString derive(EndDate = toDate(EndDate, 'dd-MM-yyyy'),",
				"          CreatedDate = toDate(CreatedDate, 'dd-MM-yyyy')) ~> DCEnddatetoDate",
				"DCEnddatetoDate sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          ProductID as string,",
				"          Name as string,",
				"          ProductNumber as string,",
				"          MakeFlag as string,",
				"          FinishedGoodsFlag as string,",
				"          Color as string,",
				"          SafetyStockLevel as string,",
				"          ReorderPoint as string,",
				"          StandardCost as string,",
				"          ListPrice as string,",
				"          Size as string,",
				"          SizeUnitMeasureCode as string,",
				"          WeightUnitMeasureCode as string,",
				"          Weight as string,",
				"          DaysToManufacture as string,",
				"          ProductLine as string,",
				"          Class as string,",
				"          Style as string,",
				"          ProductSubcategoryID as string,",
				"          ProductModelID as string,",
				"          SellStartDate as string,",
				"          SellEndDate as string,",
				"          DiscontinuedDate as string,",
				"          rowguid as string,",
				"          ModifiedDate as string",
				"     ),",
				"     partitionFileNames:['MaterialMaster.csv'],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     partitionBy('hash', 1)) ~> SNKMaterial",
				"DCtoDate sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          ProductID as string,",
				"          Name as string,",
				"          ProductNumber as string,",
				"          MakeFlag as string,",
				"          FinishedGoodsFlag as string,",
				"          Color as string,",
				"          SafetyStockLevel as string,",
				"          ReorderPoint as string,",
				"          StandardCost as string,",
				"          ListPrice as string,",
				"          Size as string,",
				"          SizeUnitMeasureCode as string,",
				"          WeightUnitMeasureCode as string,",
				"          Weight as string,",
				"          DaysToManufacture as string,",
				"          ProductLine as string,",
				"          Class as string,",
				"          Style as string,",
				"          ProductSubcategoryID as string,",
				"          ProductModelID as string,",
				"          SellStartDate as string,",
				"          SellEndDate as string,",
				"          DiscontinuedDate as string,",
				"          rowguid as string,",
				"          ModifiedDate as string",
				"     ),",
				"     partitionFileNames:['ProductionPlan.csv'],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     partitionBy('hash', 1)) ~> SNKPlan"
			]
		}
	}
}