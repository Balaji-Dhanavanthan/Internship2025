			"scriptLines": [
				"source(output(",
				"          EmployeeID as string,",
				"          Name as string,",
				"          Gender as string,",
				"          DateOfBirth as string,",
				"          Department as string,",
				"          JobTitle as string,",
				"          Salary as string,",
				"          HireDate as string,",
				"          Address as string,",
				"          Email as string,",
				"          PhoneNumber as string,",
				"          MaritalStatus as string,",
				"          EffectiveDate as string,",
				"          EndDate as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source",
				"source(output(",
				"          EmployeeID as string,",
				"          Name as string,",
				"          Gender as string,",
				"          DateOfBirth as string,",
				"          Department as string,",
				"          JobTitle as string,",
				"          Salary as string,",
				"          HireDate as string,",
				"          Address as string,",
				"          Email as string,",
				"          PhoneNumber as string,",
				"          MaritalStatus as string,",
				"          EffectiveDate as string,",
				"          EndDate as string,",
				"          flag as string,",
				"          createBy as string,",
				"          createdtime as string,",
				"          modifiedBy as string,",
				"          modifiedTime as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> target",
				"castTGT select(mapColumn(",
				"          {s.EmplyeeID} = EmployeeID,",
				"          {s.Name} = Name,",
				"          {s.Gender} = Gender,",
				"          {s.DateOfBirth} = DateOfBirth,",
				"          {s.JobTitle} = JobTitle,",
				"          {s.Salary} = Salary,",
				"          {s.HireDate} = HireDate,",
				"          {s.Address} = Address,",
				"          {s.Email} = Email,",
				"          {s.PhoneNumber} = PhoneNumber,",
				"          {s.MaritalStatus} = MaritalStatus,",
				"          {s.EffectiveDate} = EffectiveDate,",
				"          {s.EndDate} = EndDate,",
				"          {s.createBy} = createBy,",
				"          {s.createdtime} = createdtime",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> Targetselect",
				"derivedColumn4 select(mapColumn(",
				"          EmployeeID,",
				"          Name,",
				"          Gender,",
				"          DateOfBirth,",
				"          Department,",
				"          JobTitle,",
				"          Salary,",
				"          HireDate,",
				"          Address,",
				"          Email,",
				"          PhoneNumber,",
				"          MaritalStatus,",
				"          EffectiveDate,",
				"          EndDate",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sourceSelect",
				"sourceSelect, derivedColumn3 join(EmployeeID == {s.EmplyeeID},",
				"     joinType:'left',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join1",
				"join1 split(isNull({s.EmplyeeID}),",
				"     disjoint: false) ~> split1@(insert, update)",
				"union3, insertedrow union(byName: true)~> union1",
				"split1@insert derive(createBy = 'I2025_JNagara1',",
				"          createdtime = currentTimestamp(),",
				"          flag = 'active') ~> derivedColumn1",
				"derivedColumn1 select(mapColumn(",
				"          EmployeeID,",
				"          Name,",
				"          Gender,",
				"          DateOfBirth,",
				"          Department,",
				"          JobTitle,",
				"          Salary,",
				"          HireDate,",
				"          Address,",
				"          Email,",
				"          PhoneNumber,",
				"          MaritalStatus,",
				"          EffectiveDate,",
				"          EndDate,",
				"          createBy,",
				"          createdtime,",
				"          flag",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> insertedrow",
				"Targetselect derive(modifiedBy = 'unk',",
				"          modifiedTime = toTimestamp('2999-12-31 00:00:00.000', 'yyyy-MM-dd HH:mm:ss.SSS'),",
				"          flag = 'active') ~> derivedColumn3",
				"target derive(EmployeeID = toInteger(EmployeeID),",
				"          DateOfBirth = toDate(DateOfBirth, 'yyyy-MM-dd'),",
				"          Salary = toDecimal(Salary, 10, 2),",
				"          PhoneNumber = toLong((replace(replace(replace(PhoneNumber, '-', ''), '+', ''), ' ', ''))),",
				"          EffectiveDate = toDate(EffectiveDate, 'yyyy-MM-dd'),",
				"          modifiedTime = toTimestamp(modifiedTime, 'yyyy-MM-dd HH:mm:ss'),",
				"          HireDate = toDate(HireDate, 'yyyy-MM-dd'),",
				"          createdtime = toTimestamp(createdtime, 'yyyy-MM-dd HH:mm:ss'),",
				"          EndDate = toDate(EndDate,'yyyy-MM-dd')) ~> castTGT",
				"source derive(EmployeeID = toInteger(EmployeeID),",
				"          DateOfBirth = toDate(DateOfBirth, 'dd-MM-yyyy'),",
				"          Salary = toDecimal(Salary, 10, 2),",
				"          HireDate = toDate(HireDate, 'dd-MM-yyyy'),",
				"          PhoneNumber = toLong((replace(replace(replace(PhoneNumber, '-', ''), '+', ''), ' ', ''))),",
				"          EffectiveDate = toDate(EffectiveDate,'dd-MM-yyyy'),",
				"          EndDate = toTimestamp(EndDate,'yyyy-MM-dd HH:mm:ss')) ~> derivedColumn4",
				"union1, select8 union(byName: true)~> union2",
				"split2@updateddddd alterRow(updateIf((EmployeeID=={s.EmplyeeID})&&(JobTitle!={s.JobTitle}||Salary!={s.Salary}))) ~> alterRow2",
				"alterRow2 select(mapColumn(",
				"          EmployeeID = {s.EmplyeeID},",
				"          Name = {s.Name},",
				"          Gender = {s.Gender},",
				"          DateOfBirth = {s.DateOfBirth},",
				"          JobTitle = {s.JobTitle},",
				"          Salary = {s.Salary},",
				"          HireDate = {s.HireDate},",
				"          Address = {s.Address},",
				"          Email = {s.Email},",
				"          Department,",
				"          PhoneNumber = {s.PhoneNumber},",
				"          MaritalStatus = {s.MaritalStatus},",
				"          EffectiveDate = {s.EffectiveDate},",
				"          EndDate = {s.EndDate},",
				"          createBy = {s.createBy},",
				"          createdtime = {s.createdtime},",
				"          modifiedBy,",
				"          modifiedTime,",
				"          flag",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select6",
				"select6 derive(flag = 'active',",
				"          EffectiveDate = currentDate()) ~> derivedColumn5",
				"derivedColumn5, derivedColumn6 union(byName: true)~> union3",
				"alterRow2 select(mapColumn(",
				"          EmployeeID,",
				"          Name,",
				"          Gender,",
				"          DateOfBirth,",
				"          Department,",
				"          JobTitle,",
				"          Salary,",
				"          HireDate,",
				"          Address,",
				"          Email,",
				"          PhoneNumber,",
				"          MaritalStatus,",
				"          EffectiveDate,",
				"          EndDate,",
				"          createBy = {s.createBy},",
				"          createdtime = {s.createdtime},",
				"          modifiedBy,",
				"          modifiedTime,",
				"          flag",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select7",
				"select7 derive(flag = 'deactive',",
				"          EndDate = currentTimestamp(),",
				"          modifiedTime = currentTimestamp(),",
				"          modifiedBy = 'I2025_jaya') ~> derivedColumn6",
				"split1@update split((EmployeeID=={s.EmplyeeID})&&(JobTitle!={s.JobTitle}||Salary!={s.Salary}),",
				"     disjoint: false) ~> split2@(updateddddd, exist)",
				"split2@exist select(mapColumn(",
				"          EmployeeID = {s.EmplyeeID},",
				"          Name = {s.Name},",
				"          Gender = {s.Gender},",
				"          DateOfBirth = {s.DateOfBirth},",
				"          JobTitle = {s.JobTitle},",
				"          Salary = {s.Salary},",
				"          HireDate = {s.HireDate},",
				"          Address = {s.Address},",
				"          Email = {s.Email},",
				"          PhoneNumber = {s.PhoneNumber},",
				"          MaritalStatus = {s.MaritalStatus},",
				"          EffectiveDate = {s.EffectiveDate},",
				"          EndDate = {s.EndDate},",
				"          createBy = {s.createBy},",
				"          createdtime = {s.createdtime},",
				"          modifiedBy,",
				"          modifiedTime,",
				"          flag",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select8",
				"union2 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     partitionFileNames:['targetSCD2.csv'],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     mapColumn(",
				"          EmployeeID,",
				"          Name,",
				"          Gender,",
				"          DateOfBirth,",
				"          Department,",
				"          JobTitle,",
				"          Salary,",
				"          HireDate,",
				"          Address,",
				"          Email,",
				"          PhoneNumber,",
				"          MaritalStatus,",
				"          EffectiveDate,",
				"          EndDate,",
				"          flag,",
				"          createBy,",
				"          createdtime,",
				"          modifiedBy,",
				"          modifiedTime",
				"     ),",
				"     partitionBy('hash', 1)) ~> sink1"
			]
		}
	}
}