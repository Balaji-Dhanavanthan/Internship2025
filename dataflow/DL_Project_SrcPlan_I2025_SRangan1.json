{
	"name": "DL_Project_SrcPlan_I2025_SRangan1",
	"properties": {
		"folder": {
			"name": "DF_TDSG_Project_Scenario"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DS_Blob",
						"type": "DatasetReference"
					},
					"name": "SrcPlan"
				},
				{
					"dataset": {
						"referenceName": "DS_new_I2025_SRangan1",
						"type": "DatasetReference"
					},
					"name": "SrcPlantwo"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DS_STTM_I2025_SRangan1",
						"type": "DatasetReference"
					},
					"name": "sinkPlan"
				},
				{
					"dataset": {
						"referenceName": "DS_STTM_I2025_SRangan1",
						"type": "DatasetReference"
					},
					"name": "sinkPlan2"
				}
			],
			"transformations": [
				{
					"name": "DCtoDate"
				},
				{
					"name": "DCchangetoDate"
				}
			],
			"scriptLines": [
				"source(output(",
				"          MaterialCode as string,",
				"          ForecastDate as date 'dd-MM-yyyy',",
				"          Plan as short,",
				"          TrialProduct as string,",
				"          IsActive as integer,",
				"          RevisionNumber as short,",
				"          RevisionComment as string,",
				"          Line as string,",
				"          CreatedOn as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> SrcPlan",
				"source(output(",
				"          MaterialCode as string,",
				"          ForecastDate as date 'dd-MM-yyyy',",
				"          Plan as short,",
				"          TrialProduct as string,",
				"          IsActive as integer,",
				"          RevisionNumber as short,",
				"          RevisionComment as string,",
				"          Line as string,",
				"          CreatedOn as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> SrcPlantwo",
				"SrcPlan derive(CreatedOn = case(CreatedOn == 'getdate()', currentDate(),toDate(null()))) ~> DCtoDate",
				"SrcPlantwo derive(CreatedOn = case(CreatedOn == 'getdate()', currentDate(), toDate(null()))) ~> DCchangetoDate",
				"DCtoDate sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     partitionFileNames:['new_ProductionPlan.csv'],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     partitionBy('hash', 1)) ~> sinkPlan",
				"DCchangetoDate sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     partitionFileNames:['new_ProductionPlan2.csv'],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     partitionBy('hash', 1)) ~> sinkPlan2"
			]
		}
	}
}