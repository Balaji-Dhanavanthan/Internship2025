{
	"name": "DF_WS2_DIM_PTNT",
	"properties": {
		"folder": {
			"name": "Workshop"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DS_CSV_test",
						"type": "DatasetReference"
					},
					"name": "PharmSrc"
				},
				{
					"dataset": {
						"referenceName": "DS_CSV_test",
						"type": "DatasetReference"
					},
					"name": "LangSrc"
				},
				{
					"dataset": {
						"referenceName": "DS_CSV_test",
						"type": "DatasetReference"
					},
					"name": "statusSrc"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "innerJoinPharmStatus"
				},
				{
					"name": "InnerJoinPrevLang"
				},
				{
					"name": "AddDefaultValuesAndAdd"
				},
				{
					"name": "AddsurrogateKey"
				},
				{
					"name": "select1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          PHRMCY_NAT_PROV_ID_NBR as string,",
				"          PHRMCY_NCPDP_NBR as string,",
				"          PTNT_CD as string,",
				"          PSCB_FIRST_NM as string,",
				"          PSCB_LAST_NM as string,",
				"          LANG_CD as string,",
				"          DEAL_ID as string,",
				"          PHRMCY_NM as string,",
				"          PHRMCY_ST_ADDR_LINE1_TXT as string,",
				"          PHRMCY_ADDR_ZIP_CD as string,",
				"          PHRMCY_ADDR_CITY_NM as string,",
				"          PHRMCY_ADDR_ST_CD as string,",
				"          PHRMCY_ADDR_ST_NAME as string,",
				"          PHRMCY_ADDR_ZNE_CD as string,",
				"          PHRMCY_ADDR_ZNE_NM as string,",
				"          PHRMCY_SYS_IVR_DTA_STRT_DT as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> PharmSrc",
				"source(output(",
				"          LANG_ID as integer,",
				"          LANG_CD as string,",
				"          LANG_DESC as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> LangSrc",
				"source(output(",
				"          deal_id as string,",
				"          deal_nbr as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> statusSrc",
				"PharmSrc, statusSrc join(PharmSrc@DEAL_ID == statusSrc@deal_id,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> innerJoinPharmStatus",
				"innerJoinPharmStatus, LangSrc join(PharmSrc@LANG_CD == LangSrc@LANG_CD,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> InnerJoinPrevLang",
				"AddsurrogateKey derive(PHRMCY_ADDR_CITY_NM = iif(PHRMCY_ADDR_CITY_NM == 'NULL', 'UNK', PHRMCY_ADDR_CITY_NM),",
				"          PHRMCY_ADDR_ST_NAME = iif(PHRMCY_ADDR_ST_NAME == 'NULL', 'UNK', PHRMCY_ADDR_ST_NAME),",
				"          PHRMCY_ADDR_ZNE_NM = iif(PHRMCY_ADDR_ZNE_NM == 'NULL', 'UNK', PHRMCY_ADDR_ZNE_NM),",
				"          PTNT_ID = toInteger(at(split(PTNT_CD, '_'), 1)),",
				"          PTNT_CD = right(PTNT_CD, 4),",
				"          PHRMCY_NAT_PROV_ID_NBR = iif(PHRMCY_NAT_PROV_ID_NBR == 'NULL', 'UNK', PHRMCY_NAT_PROV_ID_NBR),",
				"          PHARMACY_NM = replace(replace(replace(PHRMCY_NM, '_', ' '), '-', ' '), \"'\", ' '),",
				"          PHARMACY_ADDR_CITY_NM = iif(PHRMCY_ADDR_CITY_NM == 'NULL', 'UNK', \r",
				"    iif(size(split(trim(PHRMCY_ADDR_CITY_NM), ' ')) > 1, \r",
				"        concat(upper(left(at(split(trim(PHRMCY_ADDR_CITY_NM), ''), 1), 1)), upper(left(at(split(trim(PHRMCY_ADDR_CITY_NM), ''), 2), 1))), \r",
				"        upper(substring(trim(PHRMCY_ADDR_CITY_NM), 1, 3))\r",
				"    )\r",
				")\r",
				",",
				"          column1 = ERROR_FUNCTION('')) ~> AddDefaultValuesAndAdd",
				"select1 keyGenerate(output(PTNT_KEY as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> AddsurrogateKey",
				"InnerJoinPrevLang select(mapColumn(",
				"          PHRMCY_NAT_PROV_ID_NBR,",
				"          PHRMCY_NCPDP_NBR,",
				"          PTNT_CD,",
				"          PSCB_FIRST_NM,",
				"          PSCB_LAST_NM,",
				"          LANG_CD = PharmSrc@LANG_CD,",
				"          DEAL_ID = PharmSrc@DEAL_ID,",
				"          PHRMCY_NM,",
				"          PHRMCY_ST_ADDR_LINE1_TXT,",
				"          PHRMCY_ADDR_ZIP_CD,",
				"          PHRMCY_ADDR_CITY_NM,",
				"          PHRMCY_ADDR_ST_CD,",
				"          PHRMCY_ADDR_ST_NAME,",
				"          PHRMCY_ADDR_ZNE_CD,",
				"          PHRMCY_ADDR_ZNE_NM,",
				"          PHRMCY_SYS_IVR_DTA_STRT_DT,",
				"          deal_id = statusSrc@deal_id,",
				"          deal_nbr,",
				"          LANG_ID,",
				"          LANG_CD = LangSrc@LANG_CD,",
				"          LANG_DESC",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1"
			]
		}
	}
}