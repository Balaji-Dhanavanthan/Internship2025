{
	"name": "XL_DS",
	"properties": {
		"linkedServiceName": {
			"referenceName": "DS_Blob_storgare",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"container": {
				"type": "string",
				"defaultValue": "test-container"
			},
			"directory": {
				"type": "string",
				"defaultValue": "Workshop_source"
			},
			"file": {
				"type": "string",
				"defaultValue": "BASK Invitation Dataset.xlsx"
			},
			"index": {
				"type": "int",
				"defaultValue": 0
			}
		},
		"annotations": [],
		"type": "Excel",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@dataset().file",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@dataset().directory",
					"type": "Expression"
				},
				"container": {
					"value": "@dataset().container",
					"type": "Expression"
				}
			},
			"sheetIndex": {
				"value": "@dataset().index",
				"type": "Expression"
			},
			"firstRowAsHeader": true
		},
		"schema": []
	}
}