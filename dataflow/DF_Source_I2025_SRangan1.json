{
	"name": "DF_Source_I2025_SRangan1",
	"properties": {
		"folder": {
			"name": "DF_TDSG_Project_Scenario"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DS_Blob",
						"type": "DatasetReference"
					},
					"name": "SrcPlan"
				},
				{
					"dataset": {
						"referenceName": "DS_STTM_I2025_SRangan1",
						"type": "DatasetReference"
					},
					"name": "SrcMaterial"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "derivedColumn1"
				},
				{
					"name": "derivedColumn2"
				},
				{
					"name": "derivedColumn3"
				}
			],
			"scriptLines": [
				"source(output(",
				"          MaterialCode as string,",
				"          ForecastDate as date,",
				"          Plan as short,",
				"          TrialProduct as integer,",
				"          IsActive as boolean,",
				"          RevisionNumber as integer,",
				"          RevisionComment as string,",
				"          Line as string,",
				"          CreatedOn as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> SrcPlan",
				"source(output(",
				"          MaterialCode as string,",
				"          CreatedDate as string,",
				"          MaterialDescription as string,",
				"          ProductVarient as string,",
				"          ProductType as string,",
				"          CustomerMaster as string,",
				"          Line as string,",
				"          ProductionLocation as string,",
				"          ProductionPlant as string,",
				"          {SalesPlant-GJ} as string,",
				"          {SalesPlant-HR} as string,",
				"          {SalesPlant-KA} as string,",
				"          IsActive as string,",
				"          StartDate as string,",
				"          EndDate as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> SrcMaterial",
				"SrcMaterial derive(each(match(type=='string'), $$ = case($$=='null', toString(null()),$$)\r",
				")) ~> derivedColumn1",
				"SrcPlan derive(each(match(), ERROR_FUNCTION('') = case(CreatedOn=='getdate()', currentDate(CreatedOn), toDate(null)))) ~> derivedColumn2",
				"derivedColumn1 derive(EndDate = toDate(EndDate, 'dd-MM-yyyy')) ~> derivedColumn3"
			]
		}
	}
}