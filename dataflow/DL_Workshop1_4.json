				"          createdby",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1 filter(InvitationStatus == 'Completed'\r",
				"&& Product_Quality < 6) ~> filter1"
