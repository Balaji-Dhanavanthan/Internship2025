			"scriptLines": [
				"source(output(",
				"          BusinessKey as string,",
				"          Email as string,",
				"          Phone as string,",
				"          Designation as string,",
				"          Salary as double,",
				"          LastModified as date",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> sourceData",
				"source(output(",
				"          EmployeeID as short,",
				"          BusinessKey as string,",
				"          Email as string,",
				"          Phone as string,",
				"          Designation as string,",
				"          Salary as double,",
				"          EffectiveDate as date,",
				"          ExpiryDate as date,",
				"          IsActive as boolean,",
				"          CreatedDate as date,",
				"          ModifiedDate as date",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> targetData",
				"sourceData select(mapColumn(",
				"          BusinessKey,",
				"          Email,",
				"          Phone,",
				"          Designation,",
				"          Salary,",
				"          LastModified",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> SourceSelect",
				"targetData select(mapColumn(",
				"          EmployeeID,",
				"          BusinessKey,",
				"          Email,",
				"          Phone,",
				"          Designation,",
				"          Salary,",
				"          EffectiveDate,",
				"          ExpiryDate,",
				"          IsActive,",
				"          CreatedDate,",
				"          ModifiedDate",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> TargetSelect",
				"SourceSelect derive(each(match(type=='string'), $$ = iif(isNull($$),'UNK',$$))) ~> SourceNullHanding",
				"TargetSelect derive(each(match(type=='string'), $$ = iif(isNull($$),'UNK',$$))) ~> TargetNullHanding",
				"SourceNullHanding, TargetNullHanding join(SourceNullHanding@BusinessKey == TargetNullHanding@BusinessKey,",
				"     joinType:'left',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> Join",
				"Join alterRow(insertIf(isNull(TargetNullHanding@BusinessKey))) ~> Insert",
				"Join alterRow(updateIf(SourceNullHanding@BusinessKey==TargetNullHanding@BusinessKey&&(SourceNullHanding@Email!=TargetNullHanding@Email && SourceNullHanding@Phone!=TargetNullHanding@Phone && SourceNullHanding@Designation!=TargetNullHanding@Designation && SourceSelect@Salary!=TargetSelect@Salary))) ~> Update",
