{
	"name": "STTM_DIM_TGT",
	"properties": {
		"folder": {
			"name": "TDSG_ADF_DataFlow"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DS_Blob",
						"type": "DatasetReference"
					},
					"name": "phrmcysrc"
				},
				{
					"dataset": {
						"referenceName": "DS_Blob",
						"type": "DatasetReference"
					},
					"name": "langsrc"
				},
				{
					"dataset": {
						"referenceName": "DS_Blob",
						"type": "DatasetReference"
					},
					"name": "statussrc"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "PhrmcyDerived"
				},
				{
					"name": "LangDerived"
				},
				{
					"name": "StatusDerived"
				}
			],
			"scriptLines": [
				"source(output(",
				"          PHRMCY_NAT_PROV_ID_NBR as string,",
				"          PHRMCY_NCPDP_NBR as string,",
				"          PTNT_CD as string,",
				"          PSCB_FIRST_NM as string,",
				"          PSCB_LAST_NM as string,",
				"          LANG_CD as string,",
				"          DEAL_ID as string,",
				"          PHRMCY_NM as string,",
				"          PHRMCY_ST_ADDR_LINE1_TXT as string,",
				"          PHRMCY_ADDR_ZIP_CD as string,",
				"          PHRMCY_ADDR_CITY_NM as string,",
				"          PHRMCY_ADDR_ST_CD as string,",
				"          PHRMCY_ADDR_ST_NAME as string,",
				"          PHRMCY_ADDR_ZNE_CD as string,",
				"          PHRMCY_ADDR_ZNE_NM as string,",
				"          PHRMCY_SYS_IVR_DTA_STRT_DT as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> phrmcysrc",
				"source(output(",
				"          LANG_ID as short,",
				"          LANG_CD as string,",
				"          LANG_DESC as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> langsrc",
				"source(output(",
				"          deal_id as string,",
				"          deal_nbr as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> statussrc",
				"phrmcysrc derive(PHRMCY_NCPDP_NBR = toInteger(PHRMCY_NCPDP_NBR),",
				"          PHRMCY_NAT_PROV_ID_NBR = toInteger(PHRMCY_NAT_PROV_ID_NBR),",
				"          PHRMCY_ADDR_ZIP_CD = toInteger(PHRMCY_ADDR_ZIP_CD),",
				"          PHRMCY_SYS_IVR_DTA_STRT_DT = toDate(PHRMCY_SYS_IVR_DTA_STRT_DT,'dd-MM-yyyy'),",
				"          each(match(type =='Integer'), $$ = iif(isNull($$),-1,$#)),",
				"          each(match(type=='string'), $$ = iif(isNull($$),'UNK',$$)),",
				"          each(match(type=='Date'), $$ = iif(isNull($#), toDate('31-12-2999'), $#))) ~> PhrmcyDerived",
				"langsrc derive() ~> LangDerived",
				"statussrc derive() ~> StatusDerived"
			]
		}
	}
}