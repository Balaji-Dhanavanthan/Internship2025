{
	"name": "dataflow1",
	"properties": {
		"folder": {
			"name": "practice"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DS_Excel1_I2025JNagara1",
						"type": "DatasetReference"
					},
					"name": "source1"
				},
				{
					"dataset": {
						"referenceName": "DS_I2025_JNagara1_blob",
						"type": "DatasetReference"
					},
					"name": "source2"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DS_I2025_JNagara1_blob",
						"type": "DatasetReference"
					},
					"name": "sink1",
					"rejectedDataLinkedService": {
						"referenceName": "DS_Blob_storgare",
						"type": "LinkedServiceReference"
					}
				}
			],
			"transformations": [
				{
					"name": "select1"
				},
				{
					"name": "surrogateKey1"
				},
				{
					"name": "derivedColumn1"
				},
				{
					"name": "derivedColumn2"
				},
				{
					"name": "union1"
				},
				{
					"name": "aggregate1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          {Survey ID} as integer,",
				"          {Feedback Program} as string,",
				"          {First name} as string,",
				"          {Last name} as string,",
				"          {Contact Name} as string,",
				"          Email as string,",
				"          {PHONE NUMBER} as integer,",
				"          {Contact Country} as string,",
				"          {Job Title} as string,",
				"          {Contact Department} as string,",
				"          Language as string,",
				"          Salutation as string,",
				"          Ship_to as long,",
				"          {Ship to} as long,",
				"          Sold_to as long,",
				"          {Sold to} as long,",
				"          {Lead Contact OD} as long,",
				"          {Lead Contact GBU/RBU} as string,",
				"          {Lead Contact SBU} as string,",
				"          {Main Industry} as string,",
				"          Region as string,",
				"          {Sales Channel} as string,",
				"          {Customer Network} as boolean,",
				"          {Lead Contact Segmentation1} as string,",
				"          {Lead Contact Segmentation2} as string,",
				"          {Invitation Status} as string,",
				"          Creationdate as string,",
				"          {Invitation date} as string,",
				"          {Local Invitation Date} as string,",
				"          {Survey status} as string,",
				"          {Expires on date} as string,",
				"          {Local Expiration Date} as string,",
				"          {Reason for Score} as string,",
				"          {Ease of doing business} as string,",
				"          {Issue Resolution} as string,",
				"          {Product Quality} as short,",
				"          {Delivery Reliability} as short,",
				"          {Understanding your business needs} as short",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source(output(",
				"          {Survey ID} as integer,",
				"          {Contact Name} as string,",
				"          Email as string,",
				"          {PHONE NUMBER} as integer,",
				"          {Job Title} as string,",
				"          {Ship to} as long,",
				"          {Sold to} as long,",
				"          {Lead Contact SBU} as string,",
				"          {Main Industry} as string,",
				"          {Lead Contact Segmentation1} as string,",
				"          {Invitation Status} as string,",
				"          Creationdate as string,",
				"          {Invitation date} as string,",
				"          {Local Invitation Date} as string,",
				"          {Survey status} as string,",
				"          {Expires on date} as string,",
				"          {Local Expiration Date} as string,",
				"          {Reason for Score} as string,",
				"          {Ease of doing business} as string,",
				"          {Issue Resolution} as string,",
				"          {Product Quality} as short,",
				"          {Delivery Reliability} as short,",
				"          {Understanding your business needs} as short,",
				"          created_by as short,",
				"          CREATE_BY as string,",
				"          created_time as timestamp,",
				"          file_name as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source2",
				"source1 select(mapColumn(",
				"          {Survey ID},",
				"          {Contact Name},",
				"          Email,",
				"          {PHONE NUMBER},",
				"          {Job Title},",
				"          {Ship to},",
				"          {Sold to},",
				"          {Lead Contact SBU},",
				"          {Main Industry},",
				"          {Lead Contact Segmentation1},",
				"          {Invitation Status},",
				"          Creationdate,",
				"          {Invitation date},",
				"          {Local Invitation Date},",
				"          {Survey status},",
				"          {Expires on date},",
				"          {Local Expiration Date},",
				"          {Reason for Score},",
				"          {Ease of doing business},",
				"          {Issue Resolution},",
				"          {Product Quality},",
				"          {Delivery Reliability},",
				"          {Understanding your business needs}",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1 keyGenerate(output(created_by as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> surrogateKey1",
				"surrogateKey1 derive(CREATE_BY = 'I2025_JNagara1',",
				"          each(match(type=='integer'), $$ = iifNull($#, 0)),",
				"          each(match(type=='string'), $$ = iifNull($$, 'UNK')),",
				"          created_time = currentTimestamp(),",
				"          file_name = 'BASK Invitation Dataset.xlsx',",
				"          first = concat(left({Contact Name}, 4),right({Job Title}, 2))) ~> derivedColumn1",
				"source2 derive(each(match(type=='integer'), $$ = iifNull($$, 0)),",
				"          each(match(type=='string'), $$ = iifNull($$, 'zzzzz'))) ~> derivedColumn2",
				"aggregate1, derivedColumn2 union(byName: true)~> union1",
				"derivedColumn1 aggregate(groupBy({Job Title}),",
				"     count = ERROR_FUNCTION('')) ~> aggregate1",
				"union1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     mapColumn(",
				"          {Survey ID} = count,",
				"          {Contact Name},",
				"          Email,",
				"          {PHONE NUMBER},",
				"          {Job Title},",
				"          {Ship to},",
				"          {Sold to},",
				"          {Lead Contact SBU},",
				"          {Main Industry},",
				"          {Lead Contact Segmentation1},",
				"          {Invitation Status},",
				"          Creationdate,",
				"          {Invitation date},",
				"          {Local Invitation Date},",
				"          {Survey status},",
				"          {Expires on date},",
				"          {Local Expiration Date},",
				"          {Reason for Score},",
				"          {Ease of doing business},",
				"          {Issue Resolution},",
				"          {Product Quality} = {Job Title},",
				"          {Delivery Reliability},",
				"          {Understanding your business needs},",
				"          created_by,",
				"          CREATE_BY,",
				"          created_time,",
				"          file_name",
				"     )) ~> sink1"
			]
		}
	}
}