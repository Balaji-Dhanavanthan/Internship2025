{
	"name": "DF_WS2_DIM_PTNT",
	"properties": {
		"folder": {
			"name": "Workshop"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DS_CSV_test",
						"type": "DatasetReference"
					},
					"name": "PharmSrc"
				},
				{
					"dataset": {
						"referenceName": "DS_CSV_test",
						"type": "DatasetReference"
					},
					"name": "LangSrc"
				},
				{
					"dataset": {
						"referenceName": "DS_CSV_test",
						"type": "DatasetReference"
					},
					"name": "statusSrc"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DS_CSV_test",
						"type": "DatasetReference"
					},
					"name": "finSink"
				}
			],
			"transformations": [
				{
					"name": "LeftJoinPharmStatus"
				},
				{
					"name": "LeftJoinPrevLang"
				},
				{
					"name": "AllTransformations"
				},
				{
					"name": "AddsurrogateKey"
				},
				{
					"name": "SelectForSurrogateKey"
				},
				{
					"name": "SelectingRequiredCols"
				},
				{
					"name": "FilterUSandPRZone"
				},
				{
					"name": "forFinalDTypeConversion"
				}
			],
			"scriptLines": [
				"source(output(",
				"          PHRMCY_NAT_PROV_ID_NBR as string,",
				"          PHRMCY_NCPDP_NBR as string,",
				"          PTNT_CD as string,",
				"          PSCB_FIRST_NM as string,",
				"          PSCB_LAST_NM as string,",
				"          LANG_CD as string,",
				"          DEAL_ID as string,",
				"          PHRMCY_NM as string,",
				"          PHRMCY_ST_ADDR_LINE1_TXT as string,",
				"          PHRMCY_ADDR_ZIP_CD as string,",
				"          PHRMCY_ADDR_CITY_NM as string,",
				"          PHRMCY_ADDR_ST_CD as string,",
				"          PHRMCY_ADDR_ST_NAME as string,",
				"          PHRMCY_ADDR_ZNE_CD as string,",
				"          PHRMCY_ADDR_ZNE_NM as string,",
				"          PHRMCY_SYS_IVR_DTA_STRT_DT as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> PharmSrc",
				"source(output(",
				"          LANG_ID as integer,",
				"          LANG_CD as string,",
				"          LANG_DESC as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> LangSrc",
				"source(output(",
				"          deal_id as string,",
				"          deal_nbr as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> statusSrc",
				"PharmSrc, statusSrc join(PharmSrc@DEAL_ID == statusSrc@deal_id,",
				"     joinType:'left',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> LeftJoinPharmStatus",
				"LeftJoinPharmStatus, LangSrc join(PharmSrc@LANG_CD == LangSrc@LANG_CD,",
				"     joinType:'left',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> LeftJoinPrevLang",
				"AddsurrogateKey derive(PTNT_ID = toInteger(at(split(PTNT_CD, '_'), 1)),",
				"          PTNT_CD = right(PTNT_CD, 4),",
				"          PHRMCY_NAT_PROV_ID_NBR = iif(isNull(PHRMCY_NAT_PROV_ID_NBR) || PHRMCY_NAT_PROV_ID_NBR == 'NULL', 'UNK', PHRMCY_NAT_PROV_ID_NBR),",
				"          PHRMCY_NM = replace(replace(replace(PHRMCY_NM, '_', ' '), '-', ' '), \"'\", ' '),",
				"          PHRMCY_ADDR_CITY_NM = iif(isNull(PHRMCY_ADDR_CITY_NM) || PHRMCY_ADDR_CITY_NM == 'NULL', 'UNK', \r",
				"    iif(size(split(trim(PHRMCY_ADDR_CITY_NM), ' ')) > 1, \r",
				"        concat(\r",
				"            upper(left(at(split(trim(PHRMCY_ADDR_CITY_NM), ' '), 1), 1)), \r",
				"            upper(left(at(split(trim(PHRMCY_ADDR_CITY_NM), ' '), 2), 1))\r",
				"        ), \r",
				"        upper(substring(trim(PHRMCY_ADDR_CITY_NM), 1, 3))\r",
				"    )\r",
				"),",
				"          PHRMCY_ADDR_ST_NAME = iif(isNull(PHRMCY_ADDR_ST_NAME) || PHRMCY_ADDR_ST_NAME == 'NULL', 'UNK', initCap(PHRMCY_ADDR_ST_NAME)),",
				"          PHRMCY_ADDR_ZNE_NM = iif(isNull(PHRMCY_ADDR_ZNE_NM) || PHRMCY_ADDR_ZNE_NM == 'NULL', 'UNK', initCap(PHRMCY_ADDR_ZNE_NM)),",
				"          PHRMCY_ADDR_ZIP_CD = iif(isNull(PHRMCY_ADDR_ZIP_CD) || PHRMCY_ADDR_ZIP_CD == 'NULL', 'UNK', \r",
				"    iif(length(PHRMCY_ADDR_ZIP_CD) < 5, \r",
				"        lpad(PHRMCY_ADDR_ZIP_CD, 5, '0'), \r",
				"        substring(PHRMCY_ADDR_ZIP_CD, 1, 5)    \r",
				")),",
				"          PSCB_EMAIL_ID = concat(\r",
				"    left(trim(PSCB_FIRST_NM), toInteger(round(length(trim(PSCB_FIRST_NM)) * 1/2))),\r",
				"    left(trim(PSCB_LAST_NM), 1),\r",
				"    '@UN.com'\r",
				"),",
				"          PHRMCY_SYS_IVR_DTA_STRT_DT = iif(isNull(PHRMCY_SYS_IVR_DTA_STRT_DT) || PHRMCY_SYS_IVR_DTA_STRT_DT == 'NULL', 'UNK',\r",
				"    toString(toDate(PHRMCY_SYS_IVR_DTA_STRT_DT,'dd-MM-yyyy'), 'dd-MMM-yyyy')\r",
				"),",
				"          PHRMCY_SYS_IVR_DTA_STRT_QTR = iif(isNull(PHRMCY_SYS_IVR_DTA_STRT_DT) || PHRMCY_SYS_IVR_DTA_STRT_DT == 'NULL', 'UNK',\r",
				"    concat(\r",
				"        iif(\r",
				"            month(toDate(PHRMCY_SYS_IVR_DTA_STRT_DT, 'dd-MM-yyyy')) <= 3, 'Q1',\r",
				"            iif(\r",
				"                month(toDate(PHRMCY_SYS_IVR_DTA_STRT_DT, 'dd-MM-yyyy')) <= 6, 'Q2',\r",
				"                iif(month(toDate(PHRMCY_SYS_IVR_DTA_STRT_DT, 'dd-MM-yyyy')) <= 9, \r",
				"                    'Q3', \r",
				"                    'Q4'\r",
				"                )\r",
				"            )\r",
				"        ),\r",
				"        '-',\r",
				"        toString(year(toDate(PHRMCY_SYS_IVR_DTA_STRT_DT, 'dd-MM-yyyy')))\r",
				"    )\r",
				"),",
				"          PHRMCY_SYS_IVR_DTA_STRT_YR = iif(isNull(PHRMCY_SYS_IVR_DTA_STRT_DT) || PHRMCY_SYS_IVR_DTA_STRT_DT == 'NULL',\r",
				"    'UNK',\r",
				"    toString(toDate(PHRMCY_SYS_IVR_DTA_STRT_DT, 'dd-MM-yyyy'), 'yyyy')\r",
				"),",
				"          PHRMCY_AGE = iif(isNull(PHRMCY_SYS_IVR_DTA_STRT_DT) || PHRMCY_SYS_IVR_DTA_STRT_DT == 'NULL', -1,\r",
				"    year(currentDate()) - year(toDate(PHRMCY_SYS_IVR_DTA_STRT_DT, 'dd-MM-yyyy'))\r",
				"),",
				"          DEAL_ID = iif(deal_nbr == 'NULL' || SelectForSurrogateKey@deal_id == 'NULL' || isNull(deal_nbr) || isNull(SelectForSurrogateKey@deal_id), 'UNK', SelectForSurrogateKey@deal_id),",
				"          DEAL_TYPE_CD = iif(SelectForSurrogateKey@deal_id == 'NULL' || isNull(SelectForSurrogateKey@deal_id), 'UNK',\r",
				"    left(deal_nbr, 2)\r",
				"),",
				"          DEAL_NUMBER = iif(isNull(deal_nbr), 'UNK',\r",
				"    substring(deal_nbr, 3, length(deal_nbr))\r",
				"),",
				"          LANG_CD = iif(isNull(LANG_CD) || LANG_CD == 'NULL', 'UNK', LANG_CD),",
				"          RCD_CRT_TMS = currentUTC(),",
				"          RCD_MOD_TMS = toDate(''),",
				"          START_DATE = currentUTC(),",
				"          END_DATE = toDate('')) ~> AllTransformations",
				"SelectForSurrogateKey keyGenerate(output(PTNT_KEY as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> AddsurrogateKey",
				"LeftJoinPrevLang select(mapColumn(",
				"          PHRMCY_NAT_PROV_ID_NBR,",
				"          PHRMCY_NCPDP_NBR,",
				"          PTNT_CD,",
				"          PSCB_FIRST_NM,",
				"          PSCB_LAST_NM,",
				"          LANG_CD = PharmSrc@LANG_CD,",
				"          DEAL_ID = PharmSrc@DEAL_ID,",
				"          PHRMCY_NM,",
				"          PHRMCY_ST_ADDR_LINE1_TXT,",
				"          PHRMCY_ADDR_ZIP_CD,",
				"          PHRMCY_ADDR_CITY_NM,",
				"          PHRMCY_ADDR_ST_CD,",
				"          PHRMCY_ADDR_ST_NAME,",
				"          PHRMCY_ADDR_ZNE_CD,",
				"          PHRMCY_ADDR_ZNE_NM,",
				"          PHRMCY_SYS_IVR_DTA_STRT_DT,",
				"          deal_id = statusSrc@deal_id,",
				"          deal_nbr,",
				"          LANG_ID,",
				"          LANG_CD = LangSrc@LANG_CD,",
				"          LANG_DESC",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> SelectForSurrogateKey",
				"FilterUSandPRZone select(mapColumn(",
				"          PTNT_KEY,",
				"          PTNT_ID,",
				"          PTNT_CD,",
				"          PHRMCY_NCPDP_NBR,",
				"          PHRMCY_NAT_PROV_ID_NBR,",
				"          PSCB_FIRST_NM,",
				"          PSCB_LAST_NM,",
				"          PHRMCY_NM,",
				"          PHRMCY_ADDR_CITY_NM,",
				"          PHRMCY_ADDR_ST_NM = PHRMCY_ADDR_ST_NAME,",
				"          PHRMCY_ADDR_ZNE_NM,",
				"          PHRMCY_ADDR_ZIP_CD,",
				"          PSCB_EMAIL_ID,",
				"          PHRMCY_SYS_IVR_DTA_STRT_DT,",
				"          PHRMCY_SYS_IVR_DTA_STRT_QTR,",
				"          PHRMCY_SYS_IVR_DTA_STRT_YR,",
				"          PHRMCY_AGE,",
				"          DEAL_ID = AllTransformations@DEAL_ID,",
				"          DEAL_TYPE_CD,",
				"          DEAL_NUMBER,",
				"          LANG_CD,",
				"          LANG_DESC,",
				"          RCD_CRT_TMS,",
				"          RCD_MOD_TMS,",
				"          START_DATE,",
				"          END_DATE",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> SelectingRequiredCols",
				"AllTransformations filter(equals(PHRMCY_ADDR_ZNE_CD, 'US') || equals(PHRMCY_ADDR_ZNE_CD, 'PR')) ~> FilterUSandPRZone",
				"SelectingRequiredCols derive(PTNT_KEY = iif(isNull(PTNT_KEY) || toString(PTNT_KEY) == 'NULL', -1, toInteger(PTNT_KEY)),",
				"          PTNT_ID = iif(isNull(PTNT_ID) || toString(PTNT_ID) == 'NULL', -1, toInteger(PTNT_ID)),",
				"          PHRMCY_NCPDP_NBR = iif(isNull(PHRMCY_NCPDP_NBR) || toString(PHRMCY_NCPDP_NBR) == 'NULL', -1, toInteger(PHRMCY_NCPDP_NBR))) ~> forFinalDTypeConversion",
				"forFinalDTypeConversion sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     partitionFileNames:['DIM_PTNT_I2025_JSenthil_v1.csv'],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     partitionBy('hash', 1)) ~> finSink"
			]
		}
	}
}