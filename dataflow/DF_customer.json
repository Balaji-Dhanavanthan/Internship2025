{
	"name": "DF_customer",
	"properties": {
		"folder": {
			"name": "I2025_MSyed"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "Excel_1",
						"type": "DatasetReference"
					},
					"name": "ADLSscorce"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "select1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          ProductID as string,",
				"          ProductName as string,",
				"          SupplierID as string,",
				"          CategoryID as string,",
				"          QuantityPerUnit as string,",
				"          UnitPrice as string,",
				"          UnitsInStock as string,",
				"          UnitsOnOrder as string,",
				"          ReorderLevel as string,",
				"          Discontinued as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> ADLSscorce",
				"ADLSscorce select(mapColumn(",
				"          ProductID,",
				"          ProductName,",
				"          SupplierID,",
				"          CategoryID,",
				"          QuantityPerUnit,",
				"          UnitPrice,",
				"          UnitsInStock,",
				"          UnitsOnOrder,",
				"          ReorderLevel,",
				"          Discontinued",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1"
			]
		}
	}
}