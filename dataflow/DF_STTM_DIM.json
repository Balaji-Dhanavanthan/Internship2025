{
	"name": "DF_STTM_DIM",
	"properties": {
		"folder": {
			"name": "Day9"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DS_Dim_STTM_source",
						"type": "DatasetReference"
					},
					"name": "phrmcy"
				},
				{
					"dataset": {
						"referenceName": "DS_Dim_STTM_source",
						"type": "DatasetReference"
					},
					"name": "Language"
				},
				{
					"dataset": {
						"referenceName": "DS_Dim_STTM_source",
						"type": "DatasetReference"
					},
					"name": "status"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "DCPHRMCYCast"
				},
				{
					"name": "join1"
				},
				{
					"name": "phrmcyName"
				},
				{
					"name": "languageName"
				},
				{
					"name": "statusName"
				},
				{
					"name": "join2"
				},
				{
					"name": "filter1"
				},
				{
					"name": "derivedColumn1"
				},
				{
					"name": "DCPhrmcyNull"
				},
				{
					"name": "DCTransition"
				}
			],
			"scriptLines": [
				"source(output(",
				"          PHRMCY_NAT_PROV_ID_NBR as string,",
				"          PHRMCY_NCPDP_NBR as string,",
				"          PTNT_CD as string,",
				"          PSCB_FIRST_NM as string,",
				"          PSCB_LAST_NM as string,",
				"          LANG_CD as string,",
				"          DEAL_ID as string,",
				"          PHRMCY_NM as string,",
				"          PHRMCY_ST_ADDR_LINE1_TXT as string,",
				"          PHRMCY_ADDR_ZIP_CD as string,",
				"          PHRMCY_ADDR_CITY_NM as string,",
				"          PHRMCY_ADDR_ST_CD as string,",
				"          PHRMCY_ADDR_ST_NAME as string,",
				"          PHRMCY_ADDR_ZNE_CD as string,",
				"          PHRMCY_ADDR_ZNE_NM as string,",
				"          PHRMCY_SYS_IVR_DTA_STRT_DT as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> phrmcy",
				"source(output(",
				"          LANG_ID as integer,",
				"          LANG_CD as string,",
				"          LANG_DESC as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> Language",
				"source(output(",
				"          deal_id as string,",
				"          deal_nbr as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> status",
				"phrmcyName derive(PHRMCY_NAT_PROV_ID_NBR = toInteger({p.PHRMCY_NAT_PROV_ID_NBR}),",
				"          PHRMCY_ADDR_ZIP_CD = toInteger({p.PHRMCY_ADDR_ZIP_CD}),",
				"          PHRMCY_SYS_IVR_DTA_STRT_DT = toDate({p.PHRMCY_SYS_IVR_DTA_STRT_DT},'dd-MM-yyyy HH:mm')) ~> DCPHRMCYCast",
				"derivedColumn1, languageName join({p.LANG_CD} == {l.LANG_CD},",
				"     joinType:'left',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join1",
				"DCPhrmcyNull select(mapColumn(",
				"          {p.PHRMCY_NAT_PROV_ID_NBR} = PHRMCY_NAT_PROV_ID_NBR,",
				"          {p.PHRMCY_NCPDP_NBR} = PHRMCY_NCPDP_NBR,",
				"          {p.PTNT_CD} = PTNT_CD,",
				"          {p.PSCB_FIRST_NM} = PSCB_FIRST_NM,",
				"          {p.PSCB_LAST_NM} = PSCB_LAST_NM,",
				"          {p.LANG_CD} = LANG_CD,",
				"          {p.DEAL_ID} = DEAL_ID,",
				"          {p.PHRMCY_NM} = PHRMCY_NM,",
				"          {p.PHRMCY_ST_ADDR_LINE1_TXT} = PHRMCY_ST_ADDR_LINE1_TXT,",
				"          {p.PHRMCY_ADDR_ZIP_CD} = PHRMCY_ADDR_ZIP_CD,",
				"          {p.PHRMCY_ADDR_CITY_NM} = PHRMCY_ADDR_CITY_NM,",
				"          {p.PHRMCY_ADDR_ST_CD} = PHRMCY_ADDR_ST_CD,",
				"          {p.PHRMCY_ADDR_ST_NAME} = PHRMCY_ADDR_ST_NAME,",
				"          {p.PHRMCY_ADDR_ZNE_CD} = PHRMCY_ADDR_ZNE_CD,",
				"          {p.PHRMCY_ADDR_ZNE_NM} = PHRMCY_ADDR_ZNE_NM,",
				"          {p.PHRMCY_SYS_IVR_DTA_STRT_DT} = PHRMCY_SYS_IVR_DTA_STRT_DT",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> phrmcyName",
				"Language select(mapColumn(",
				"          {l.LANG_ID} = LANG_ID,",
				"          {l.LANG_CD} = LANG_CD,",
				"          {l.LANG_DESC} = LANG_DESC",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> languageName",
				"status select(mapColumn(",
				"          {d.deal_id} = deal_id,",
				"          {d.deal_nbr} = deal_nbr",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> statusName",
				"join1, statusName join({p.DEAL_ID} == {d.deal_id}",
				"     && {p.DEAL_ID} == {d.deal_nbr},",
				"     joinType:'left',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join2",
				"join2 filter( in( ['US','PR'],{p.PHRMCY_ADDR_ZNE_CD})) ~> filter1",
				"DCPHRMCYCast derive(each(match(type=='string'), $$ = iif(isNull($$),'UNK', $$)),",
				"          each(match(type=='integer'), $$ = iifNull(-1, $#)),",
				"          each(match(type=='date'), $$ = iifNull('01-01-1900', $0))) ~> derivedColumn1",
				"phrmcy derive(each(match(type=='string'), $$ = case($$=='NULL', toString(null()), $$))) ~> DCPhrmcyNull",
				"filter1 derive({p.PTNT_CD} = replace({p.PTNT_CD}, '_', ''),",
				"          PTNT_CD = {p.PTNT_CD},",
				"          PHRMCY_NCPDP_NBR = ERROR_FUNCTION(''),",
				"          PHRMCY_NAT_PROV_ID_NBR = ERROR_FUNCTION(''),",
				"          PSCB_FIRST_NM = ERROR_FUNCTION(''),",
				"          PSCB_LAST_NM = ERROR_FUNCTION(''),",
				"          PHRMCY_NM = ERROR_FUNCTION(''),",
				"          {PHRMCY_ADDR_CITY_NM } = ERROR_FUNCTION(''),",
				"          PHRMCY_ADDR_ST_NM = ERROR_FUNCTION(''),",
				"          PHRMCY_ADDR_ZNE_NM = ERROR_FUNCTION(''),",
				"          {PHRMCY_ADDR_ZIP_CD } = ERROR_FUNCTION(''),",
				"          PSCB_EMAIL_ID = ERROR_FUNCTION(''),",
				"          PHRMCY_SYS_IVR_DTA_STRT_DT = ERROR_FUNCTION(''),",
				"          PHRMCY_SYS_IVR_DTA_STRT_QTR = ERROR_FUNCTION(''),",
				"          PHRMCY_SYS_IVR_DTA_STRT_YR = ERROR_FUNCTION(''),",
				"          PHRMCY_AGE = ERROR_FUNCTION(''),",
				"          DEAL_ID = ERROR_FUNCTION(''),",
				"          DEAL_TYPE_CD = ERROR_FUNCTION(''),",
				"          DEAL_NUMBER = ERROR_FUNCTION(''),",
				"          LANG_CD = ERROR_FUNCTION(''),",
				"          LANG_DESC = ERROR_FUNCTION('')) ~> DCTransition"
			]
		}
	}
}