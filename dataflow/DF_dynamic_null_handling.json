{
	"name": "DF_dynamic_null_handling",
	"properties": {
		"folder": {
			"name": "Supply Chain"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DS_EXCEL_BLOB",
						"type": "DatasetReference"
					},
					"name": "SRCexcel"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DS_CSV_test",
						"type": "DatasetReference"
					},
					"name": "SINKcsv"
				}
			],
			"transformations": [
				{
					"name": "DynamicNullHandling"
				}
			],
			"scriptLines": [
				"parameters{",
				"     output_filename as string",
				"}",
				"source(output(",
				"          Division as string,",
				"          PORNumber as string,",
				"          PORValue as double,",
				"          AdditionalCharge as double,",
				"          PORRequester as string,",
				"          RingiNO as string,",
				"          SpendDetails as string,",
				"          SAPPR as long,",
				"          PONumber as long,",
				"          PODate as date,",
				"          DeliveryDate as date,",
				"          VendorCode as string,",
				"          VendorName as string,",
				"          TotalPOValue as double,",
				"          Currency as string,",
				"          ExchangeRate as double,",
				"          {POValue(INR)} as double,",
				"          CreatedBy as short,",
				"          PipelineRunDate as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> SRCexcel",
				"SRCexcel derive(each(match(type=='string'), $$ = iif(lower($$) == 'null', toString(null()), $$))) ~> DynamicNullHandling",
				"DynamicNullHandling sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     partitionFileNames:[($output_filename)],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     partitionBy('hash', 1)) ~> SINKcsv"
			]
		}
	}
}