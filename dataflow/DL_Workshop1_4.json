				"     ignoreNoFilesFound: false) ~> source1",
				"source1 select(mapColumn(",
				"          Survey_ID,",
				"          ContactName,",
				"          Email,",
				"          PHONE_NUMBER,",
				"          Job_Title,",
				"          Ship_to,",
				"          Sold_to,",
				"          Lead_Contact_SBU,",
				"          MainIndustry,",
				"          Lead_Contact_Segmentation1,",
				"          InvitationStatus,",
				"          Creationdate,",
				"          Invitationdate,",
				"          Local_Invitation_Date,",
				"          Survey_status,",
				"          Expires_on_date,",
				"          Local_Expiration_Date,",
