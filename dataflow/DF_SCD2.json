{
	"name": "DF_SCD2",
	"properties": {
		"folder": {
			"name": "day5SCD"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DS_Source",
						"type": "DatasetReference"
					},
					"name": "source"
				},
				{
					"dataset": {
						"referenceName": "DS_CSV_Sink",
						"type": "DatasetReference"
					},
					"name": "target"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DS_CSV_Sink",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "Targetselect"
				},
				{
					"name": "sourceSelect"
				},
				{
					"name": "LeftJoinSourceTarget"
				},
				{
					"name": "SliptInsertUdate"
				},
				{
					"name": "unionInsert"
				},
				{
					"name": "FlagForInsert"
				},
				{
					"name": "SelectSourceColumn"
				},
				{
					"name": "setFlag"
				},
				{
					"name": "castTGT"
				},
				{
					"name": "castSource"
				},
				{
					"name": "unionExist"
				},
				{
					"name": "UpdateBranch"
				},
				{
					"name": "selectSourceForUpdate"
				},
				{
					"name": "ActiveAndDate"
				},
				{
					"name": "unionUpdate"
				},
				{
					"name": "selectTargetColumn"
				},
				{
					"name": "DeactiveFlag"
				},
				{
					"name": "split2"
				},
				{
					"name": "TargetColumns"
				},
				{
					"name": "sort"
				}
			],
			"scriptLines": [
				"source(output(",
				"          EmployeeID as string,",
				"          Name as string,",
				"          Gender as string,",
				"          DateOfBirth as string,",
				"          Department as string,",
				"          JobTitle as string,",
				"          Salary as string,",
				"          HireDate as string,",
				"          Address as string,",
				"          Email as string,",
				"          PhoneNumber as string,",
				"          MaritalStatus as string,",
				"          EffectiveDate as string,",
				"          EndDate as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source",
				"source(output(",
				"          EmployeeID as string,",
				"          Name as string,",
				"          Gender as string,",
				"          DateOfBirth as string,",
				"          Department as string,",
				"          JobTitle as string,",
				"          Salary as string,",
				"          HireDate as string,",
				"          Address as string,",
				"          Email as string,",
				"          PhoneNumber as string,",
				"          MaritalStatus as string,",
				"          EffectiveDate as string,",
				"          EndDate as string,",
				"          flag as string,",
				"          createBy as string,",
				"          createdtime as string,",
				"          modifiedBy as string,",
				"          modifiedTime as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> target",
				"castTGT select(mapColumn(",
				"          {s.EmplyeeID} = EmployeeID,",
				"          {s.Name} = Name,",
				"          {s.Gender} = Gender,",
				"          {s.DateOfBirth} = DateOfBirth,",
				"          {s.JobTitle} = JobTitle,",
				"          {s.Salary} = Salary,",
				"          {s.HireDate} = HireDate,",
				"          {s.Address} = Address,",
				"          {s.Email} = Email,",
				"          {s.PhoneNumber} = PhoneNumber,",
				"          {s.MaritalStatus} = MaritalStatus,",
				"          {s.EffectiveDate} = EffectiveDate,",
				"          {s.EndDate} = EndDate,",
				"          {s.createBy} = createBy,",
				"          {s.createdtime} = createdtime",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> Targetselect",
				"castSource select(mapColumn(",
				"          EmployeeID,",
				"          Name,",
				"          Gender,",
				"          DateOfBirth,",
				"          Department,",
				"          JobTitle,",
				"          Salary,",
				"          HireDate,",
				"          Address,",
				"          Email,",
				"          PhoneNumber,",
				"          MaritalStatus,",
				"          EffectiveDate,",
				"          EndDate",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sourceSelect",
				"sourceSelect, setFlag join(EmployeeID == {s.EmplyeeID},",
				"     joinType:'left',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> LeftJoinSourceTarget",
				"LeftJoinSourceTarget split(isNull({s.EmplyeeID}),",
				"     disjoint: false) ~> SliptInsertUdate@(insert, updateAndExisting)",
				"unionUpdate, SelectSourceColumn union(byName: true)~> unionInsert",
				"SliptInsertUdate@insert derive(createBy = 'I2025_JNagara1',",
				"          createdtime = currentTimestamp(),",
				"          flag = 'active') ~> FlagForInsert",
				"FlagForInsert select(mapColumn(",
				"          EmployeeID,",
				"          Name,",
				"          Gender,",
				"          DateOfBirth,",
				"          Department,",
				"          JobTitle,",
				"          Salary,",
				"          HireDate,",
				"          Address,",
				"          Email,",
				"          PhoneNumber,",
				"          MaritalStatus,",
				"          EffectiveDate,",
				"          EndDate,",
				"          createBy,",
				"          createdtime,",
				"          modifiedBy,",
				"          modifiedTime,",
				"          flag",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> SelectSourceColumn",
				"Targetselect derive(modifiedBy = 'unk',",
				"          modifiedTime = toTimestamp('2999-12-31 00:00:00.000', 'yyyy-MM-dd HH:mm:ss.SSS'),",
				"          flag = 'active') ~> setFlag",
				"target derive(EmployeeID = toInteger(EmployeeID),",
				"          DateOfBirth = toDate(DateOfBirth, 'yyyy-MM-dd'),",
				"          Salary = toDecimal(Salary, 10, 2),",
				"          PhoneNumber = toLong((replace(replace(replace(PhoneNumber, '-', ''), '+', ''), ' ', ''))),",
				"          EffectiveDate = toDate(EffectiveDate, 'yyyy-MM-dd'),",
				"          modifiedTime = toTimestamp(modifiedTime, 'yyyy-MM-dd HH:mm:ss'),",
				"          HireDate = toDate(HireDate, 'yyyy-MM-dd'),",
				"          createdtime = toTimestamp(createdtime, 'yyyy-MM-dd HH:mm:ss'),",
				"          EndDate = toDate(EndDate,'yyyy-MM-dd')) ~> castTGT",
				"source derive(EmployeeID = toInteger(EmployeeID),",
				"          DateOfBirth = toDate(DateOfBirth, 'dd-MM-yyyy'),",
				"          Salary = toDecimal(Salary, 10, 2),",
				"          HireDate = toDate(HireDate, 'dd-MM-yyyy'),",
				"          PhoneNumber = toLong((replace(replace(replace(PhoneNumber, '-', ''), '+', ''), ' ', ''))),",
				"          EffectiveDate = toDate(EffectiveDate,'dd-MM-yyyy'),",
				"          EndDate = toDate(EndDate,'dd-MM-yyyy')) ~> castSource",
				"unionInsert, TargetColumns union(byName: true)~> unionExist",
				"split2@update alterRow(updateIf((EmployeeID=={s.EmplyeeID})&&(JobTitle!={s.JobTitle}||Salary!={s.Salary}))) ~> UpdateBranch",
				"UpdateBranch select(mapColumn(",
				"          EmployeeID = {s.EmplyeeID},",
				"          Name = {s.Name},",
				"          Gender = {s.Gender},",
				"          DateOfBirth = {s.DateOfBirth},",
				"          JobTitle = {s.JobTitle},",
				"          Salary = {s.Salary},",
				"          HireDate = {s.HireDate},",
				"          Address = {s.Address},",
				"          Email = {s.Email},",
				"          Department,",
				"          PhoneNumber = {s.PhoneNumber},",
				"          MaritalStatus = {s.MaritalStatus},",
				"          EffectiveDate = {s.EffectiveDate},",
				"          EndDate = {s.EndDate},",
				"          createBy = {s.createBy},",
				"          createdtime = {s.createdtime},",
				"          modifiedBy,",
				"          modifiedTime,",
				"          flag",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> selectSourceForUpdate",
				"selectSourceForUpdate derive(flag = 'active',",
				"          EffectiveDate = currentDate()) ~> ActiveAndDate",
				"ActiveAndDate, DeactiveFlag union(byName: true)~> unionUpdate",
				"UpdateBranch select(mapColumn(",
				"          EmployeeID,",
				"          Name,",
				"          Gender,",
				"          DateOfBirth,",
				"          Department,",
				"          JobTitle,",
				"          Salary,",
				"          HireDate,",
				"          Address,",
				"          Email,",
				"          PhoneNumber,",
				"          MaritalStatus,",
				"          EffectiveDate,",
				"          EndDate,",
				"          createBy = {s.createBy},",
				"          createdtime = {s.createdtime},",
				"          modifiedBy,",
				"          modifiedTime,",
				"          flag",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> selectTargetColumn",
				"selectTargetColumn derive(flag = 'deactive',",
				"          EndDate = currentDate(),",
				"          modifiedTime = currentTimestamp(),",
				"          modifiedBy = 'I2025_jaya') ~> DeactiveFlag",
				"SliptInsertUdate@updateAndExisting split((EmployeeID=={s.EmplyeeID})&&(JobTitle!={s.JobTitle}||Salary!={s.Salary}),",
				"     disjoint: false) ~> split2@(update, exist)",
				"split2@exist select(mapColumn(",
				"          EmployeeID = {s.EmplyeeID},",
				"          Name = {s.Name},",
				"          Gender = {s.Gender},",
				"          DateOfBirth = {s.DateOfBirth},",
				"          JobTitle = {s.JobTitle},",
				"          Salary = {s.Salary},",
				"          HireDate = {s.HireDate},",
				"          Address = {s.Address},",
				"          Department,",
				"          Email = {s.Email},",
				"          PhoneNumber = {s.PhoneNumber},",
				"          MaritalStatus = {s.MaritalStatus},",
				"          EffectiveDate = {s.EffectiveDate},",
				"          EndDate = {s.EndDate},",
				"          createBy = {s.createBy},",
				"          createdtime = {s.createdtime},",
				"          modifiedBy,",
				"          modifiedTime,",
				"          flag",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> TargetColumns",
				"unionExist sort(asc(EmployeeID, true)) ~> sort",
				"sort sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     partitionFileNames:['targetSCD2.csv'],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     mapColumn(",
				"          EmployeeID,",
				"          Name,",
				"          Gender,",
				"          DateOfBirth,",
				"          Department,",
				"          JobTitle,",
				"          Salary,",
				"          HireDate,",
				"          Address,",
				"          Email,",
				"          PhoneNumber,",
				"          MaritalStatus,",
				"          EffectiveDate,",
				"          EndDate,",
				"          flag,",
				"          createBy,",
				"          createdtime,",
				"          modifiedBy,",
				"          modifiedTime",
				"     ),",
				"     partitionBy('hash', 1)) ~> sink1"
			]
		}
	}
}