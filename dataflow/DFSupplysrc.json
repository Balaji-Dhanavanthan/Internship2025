{
	"name": "DFSupplysrc",
	"properties": {
		"folder": {
			"name": "SupplyChain"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DS_Excel_I2025_JNagar1",
						"type": "DatasetReference"
					},
					"name": "STGManageHistoryMaster"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DS_CSV_I2025_JNagara1",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "derivedColumn4"
				}
			],
			"scriptLines": [
				"parameters{",
				"     outputfile as string",
				"}",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> STGManageHistoryMaster",
				"STGManageHistoryMaster derive(each(match(type=='string'), $$ = case($$=='NULL', toString(null()), $$))) ~> derivedColumn4",
				"derivedColumn4 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     partitionFileNames:[($outputfile)],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     partitionBy('hash', 1)) ~> sink1"
			]
		}
	}
}