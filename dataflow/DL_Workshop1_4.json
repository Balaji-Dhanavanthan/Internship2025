				"          Ship_to as string,",
				"          Sold_to as string,",
				"          Lead_Contact_OD as string,",
				"          Lead_Contact_GBU_RBU as string,",
				"          Lead_Contact_SBU as string,",
				"          MainIndustry as string,",
				"          Region as string,",
				"          SalesChannel as string,",
				"          Customer_Network as boolean,",
				"          Lead_Contact_Segmentation1 as string,",
