{
	"name": "DF_ADF_Assessment_Qn2",
	"properties": {
		"folder": {
			"name": "ADF_Assessment_DataFlow"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DS_Blob",
						"type": "DatasetReference"
					},
					"name": "importAnime1"
				},
				{
					"dataset": {
						"referenceName": "DS_excel",
						"type": "DatasetReference"
					},
					"name": "importAnime2"
				},
				{
					"dataset": {
						"referenceName": "DS_Blob",
						"type": "DatasetReference"
					},
					"name": "importAnime3"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "mergeAnime1n2"
				},
				{
					"name": "mergeAnime3"
				}
			],
			"scriptLines": [
				"source(output(",
				"          anime_id as integer,",
				"          name as string,",
				"          genre as string,",
				"          type as string,",
				"          episodes as string,",
				"          rating as double,",
				"          members as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> importAnime1",
				"source(output(",
				"          anime_id as integer,",
				"          name as string,",
				"          genre as string,",
				"          type as string,",
				"          episodes as string,",
				"          rating as double,",
				"          members as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> importAnime2",
				"source(output(",
				"          anime_id as integer,",
				"          name as string,",
				"          genre as string,",
				"          type as string,",
				"          episodes as string,",
				"          rating as double,",
				"          members as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> importAnime3",
				"importAnime1, importAnime2 union(byName: true)~> mergeAnime1n2",
				"mergeAnime1n2, importAnime3 union(byName: true)~> mergeAnime3"
			]
		}
	}
}